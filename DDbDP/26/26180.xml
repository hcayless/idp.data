    <lb n="4"/>κολοφόνεια <num value="3">γ</num>. 

    <lb n="5"/><unclear>Κό</unclear>μαρος <app type="editorial"><lem resp="Clarysse and Devijver, ZPE 78 (1989) 297ff"><choice><reg>Χεμβήκει</reg><orig>Χινβήκ<supplied reason="lost">ει</supplied></orig></choice></lem><rdg><expan>χιλ<ex>ίαρχος</ex></expan> Βήκ<supplied reason="lost">ει</supplied></rdg></app>

    <lb n="6"/><gap reason="lost" quantity="3" unit="character"/><gap reason="illegible" quantity="1" unit="character"/>ρεσενουφι<gap reason="illegible" quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/> 

